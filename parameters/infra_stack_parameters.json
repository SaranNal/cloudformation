{
  "SSLCertificateID": "$SSLCertificateID",
  "VPCId": "$VPCId",
  "PublicSubnets": "$PublicSubnets",
  "PrivateSubnets": "$PrivateSubnets",
  "HTTPSListener1": "$HTTPSListener1",
  "HTTPSListener2": "$HTTPSListener2",
  "SecurityGroup": "$SecurityGroup",
  "INFRA_STACK_PARAMETERS": [
    {"ParameterKey": "SSLCertificateID", "ParameterValue": "$SSLCertificateID"},
    {"ParameterKey": "VPCId", "ParameterValue": "$VPCId"},
    {"ParameterKey": "PublicSubnets", "ParameterValue": "$(IFS=,; echo "${PublicSubnets[*]}")"},
    {"ParameterKey": "PrivateSubnets", "ParameterValue": "$(IFS=,; echo "${PrivateSubnets[*]}")"},
    {"ParameterKey": "HTTPSListener1", "ParameterValue": "$HTTPSListener1"},
    {"ParameterKey": "HTTPSListener2", "ParameterValue": "$HTTPSListener2"},
    {"ParameterKey": "SecurityGroup", "ParameterValue": "$SecurityGroup"}
  ]
}
